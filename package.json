  "main": "dist/index.js",
  "scripts": {
    "start": "npm run init && npm run bot && npm run stop",
    "init": "npx tsc && node dist/init.js",
    "bot": "npx tsc && node dist/bot.js",
    "stop": "npx tsc && node dist/stop.js",
    "test": "npx tsc && node dist/test.js",
    "serve": "npm run api",
    "api": "npx tsc && node dist/server.js",
    "api:dev": "npx tsc && nodemon dist/server.js",
    "prettier": "prettier --write '**/*.{js,ts,json,md}'",
    "db:up": "docker compose up -d mysql",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs mysql",
    "db:reset": "docker compose down -v && docker-compose up -d mysql",
    "build": "npx tsc"
  },
